public class Main
{
    public static void revofstr(String str,int ind)
    {
        if(ind==0){
            System.out.print(str.charAt(ind));
            return;
        }
        
        System.out.print(str.charAt(ind));
        revofstr(str,ind-1);
    }
	public static void main(String[] args) {
		String str="immu";
		revofstr(str,str.length()-1);
		
	}
}


