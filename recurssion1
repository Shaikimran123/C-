public class Main
{
    
    public static void printnaturalnum(int n){
        if(n==0){
            return;
        }
        System.out.println(n);
        printnaturalnum(n-1);
        
    }
	public static void main(String[] args) {
    int n=5;
    printnaturalnum(n);
	}
}
output-->5 4 3 2 1
